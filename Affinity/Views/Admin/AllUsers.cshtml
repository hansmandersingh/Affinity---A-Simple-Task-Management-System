@model List<Affinity.Models.ApplicationUser>
@{
    ViewBag.Title = "AllUsers";
}

<div class="jumbotron">
    <h1>All Users</h1>
</div>

<div class="row">
    @foreach (var user in Model)
    {
        <div class="col-md-4">
            <p style="font-size:medium">@user.UserName</p>
        </div>
    }
</div>