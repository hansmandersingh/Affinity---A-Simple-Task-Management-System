
@{
    ViewBag.Title = "AssignATaskByDeveloper";
}

<h2>Assign A Task By Developer</h2>

@using (Html.BeginForm())
{
    <h4>Select task from the List :</h4> @Html.DropDownList("taskId", new SelectList(ViewBag.taskList, "Id", "Name", "Id"))
    <h4>Select User from the List :</h4> @Html.DropDownList("userId", new SelectList(ViewBag.UserList, "Id", "UserName", "Id"))

    <input type="submit" value="add" name="save"/>
}
